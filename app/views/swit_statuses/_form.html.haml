= form_for @swit_status do |f|
  - if @swit_status.errors.any?
    #error_explanation
      %h2= "#{pluralize(@swit_status.errors.count, "error")} prohibited this swit_status from being saved:"
      %ul
        - @swit_status.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :swit_id
    = f.number_field :swit_id
  .field
    = f.label :status
    = f.number_field :status
  .actions
    = f.submit 'Save'
